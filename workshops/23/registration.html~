<!-- Begin
var empty = new Image(); empty.src = "./images/fieldempty.gif";

var haveerrors = 0;
function showImage(imagename, imageurl, errors) {
document[imagename].src = imageurl;
if (!haveerrors && errors) haveerrors = errors;
}

function validate(f) {
haveerrors = 0;
(f.first_name.value.length < 2) 
? showImage("first_nameerror", "./images/fieldempty.gif", true)
: showImage("first_nameerror", "./images/blank.gif", false);

(f.last_name.value.length < 3) 
? showImage("last_nameerror", "./images/fieldempty.gif", true)
: showImage("last_nameerror", "./images/blank.gif", false);

(f.institution.value.length < 3) 
? showImage("institutionerror", "./images/fieldempty.gif", true)
: showImage("institutionerror", "./images/blank.gif", false);

(f.email.value.length < 4) 
? showImage("emailerror", "./images/fieldempty.gif", true)
: showImage("emailerror", "./images/blank.gif", false);

(f.address1.value.length < 5) 
? showImage("addresserror", "./images/fieldempty.gif", true)
: showImage("addresserror", "./images/blank.gif", false);

(f.city.value.length < 3) 
? showImage("cityerror", "./images/fieldempty.gif", true)
: showImage("cityerror", "./images/blank.gif", false);


return (!haveerrors);
}

//  End -->


